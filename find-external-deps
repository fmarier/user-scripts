#!/bin/sh

# Rust (https://crates.io/)
find . -name "Cargo.toml"

# Python (https://pypi.org/)
find . -name "requirements.txt"
find . -name "requirements3.txt"

# Ansible (https://galaxy.ansible.com/)
find . -name "requirements.yml"

# JavaScript / node.js (https://www.npmjs.com/)
find . -name "package.json"

# Crystal (https://shardbox.org/)
find . -name "shard.yml"
